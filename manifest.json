{
	"manifest_version": 2,

	"name": "PASS2PolyRatings",
	"description": "Link teacher names on PASS to PolyRatings.",
	"version": "1.3",
	"permissions": [
		"webRequest",
		"*://*.polyratings.com/"
    ],
    "icons": { "48": "logo48.png",
      "128": "logo128.png" },
	"content_scripts": 
	[
		{
			"matches": ["http://pass.calpoly.edu/main.html"],
			"js": ["linker.js"],
			"run_at": "document_end"
		}
	]
}
